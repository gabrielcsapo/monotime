{
    "tags": {
        "allowUnknownTags": true,
        "dictionaries": ["jsdoc"]
    },
    "source": {
        "include": ["./", "README.md"],
        "includePattern": ".js$",
        "excludePattern": "(node_modules/|docs|test)"
    },
    "plugins": [
        "plugins/markdown"
    ],
    "templates": {
        "cleverLinks": true,
        "monospaceLinks": true,
        "useLongnameInNav": false
    },
    "opts": {
        "destination": "./documentation",
        "encoding": "utf8",
        "private": false,
        "recurse": true,
        "template": "./node_modules/minami"
    }
}
